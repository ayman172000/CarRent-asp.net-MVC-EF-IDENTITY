@model IEnumerable<CarRentt.Models.ApplicationUser>

@{
    ViewBag.Title = "Index";
}


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ImageCinRecto)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ImagePermisRecto)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ImageCinVerso)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ImagePermisVerso)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PhoneNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.etat)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        <img src="~/image/cin/@item.ImageCinRecto" style="height:120px;width:120px;"/>
    </td>
    <td>
        <img src="~/image/cin/@item.ImagePermisRecto" style="height:120px;width:120px;" />
    </td>
    <td>
        <img src="~/image/cin/@item.ImageCinVerso" style="height:120px;width:120px;" />
    </td>
    <td>
        <img src="~/image/cin/@item.ImagePermisVerso" style="height:120px;width:120px;" />
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Email)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.PhoneNumber)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.UserName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.etat)
    </td>
    <td>
        @Html.ActionLink("Valider", "valider", new { id = item.Id }) |
        @Html.ActionLink("Details", "Details", new { id = item.Id }) |
        @Html.ActionLink("Bloquer", "Bloquer", new { id = item.Id })
    </td>
</tr>
}

</table>
